{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "ES5"],
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "downlevelIteration": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
